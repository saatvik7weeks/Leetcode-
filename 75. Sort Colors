class Solution {
public:
    void sortColors(vector<int>& nums) 
{   
   int ctr=0;
    while (ctr!=nums.size())
    {
    for(int i=0;i<nums.size()-1;i++)
    {
        if (nums[i]>nums[i+1])
        {
            swap(nums[i+1],nums[i]);
        }
    }
    ctr++;
   if(ctr==nums.size())
   {
       return;
   } 
 }
}

        
    
};
